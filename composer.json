        "symfony/http-foundation": "^4.1",
        "illuminate/config": "~5.7|~5.8|^6.0",
        "symfony/serializer": "4.3.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ahmetbedir",
